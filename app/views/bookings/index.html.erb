<p style="color: green"><%= notice %></p>

<h1>Bookings</h1>

<div style="display:flex; align-center; margin:10px; flex-direction: column;" id="bookings">
  <% @bookings.each do |booking| %>
    <%= render booking %>
    <p>
      <%= link_to "Show this booking", booking %>
    </p>
  <% end %>
</div>

<%= link_to "New booking", new_booking_path %>
